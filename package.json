{
  "name": "vulpes-ui",
  "version": "0.6.1",
  "description": "Web-based GUI for Vulpes",
  "main": "source/index.js",
  "scripts": {
    "build": "webpack --progress --mode production --config webpack.config.js",
    "build:development": "webpack --watch --progress --mode development --config webpack.config.js",
    "dev": "RELOAD=yes concurrently --kill-others -n 'server,webpack' 'node ./scripts/dev.js' 'npm run build:development'",
    "format": "prettier --write '{source,test}/**/*.js'",
    "precommit": "lint-staged",
    "prepublishOnly": "npm run build",
    "test": "run-s test:unit test:integration test:format",
    "test:format": "prettier-check '{source,test}/**/*.js'",
    "test:integration": "mocha -r test/index.js 'test/integration/**/*.spec.js'",
    "test:unit": "mocha -r test/index.js 'test/unit/**/*.spec.js'",
    "watch": "npm run build:development"
  },
  "files": [
    "dist/**/*",
    "source/*.js"
  ],
  "lint-staged": {
    "{source,test}/**/*.js": [
      "prettier --write",
      "git add"
    ]
  },
  "engines": {
    "node": ">=8"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Kiosked/vulpes-ui.git"
  },
  "keywords": [
    "vulpes",
    "job",
    "management",
    "queue",
    "task",
    "gui"
  ],
  "author": "Perry Mitchell <perry@kiosked.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Kiosked/vulpes-ui/issues"
  },
  "homepage": "https://github.com/Kiosked/vulpes-ui#readme",
  "devDependencies": {
    "@babel/core": "^7.3.4",
    "@babel/plugin-proposal-class-properties": "^7.3.4",
    "@babel/plugin-proposal-function-bind": "^7.2.0",
    "@babel/preset-env": "^7.3.4",
    "@babel/preset-react": "^7.0.0",
    "@blueprintjs/core": "^3.14.1",
    "axios": "^0.18.0",
    "babel-loader": "^8.0.5",
    "babel-plugin-jsx-control-statements": "^3.2.8",
    "brace": "^0.11.1",
    "chai": "^4.2.0",
    "concurrently": "^4.1.0",
    "copy-webpack-plugin": "^4.6.0",
    "css-loader": "^1.0.1",
    "cytoscape": "3.2.5",
    "express": "^4.16.4",
    "file-loader": "^3.0.1",
    "history": "^4.7.2",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.5",
    "lodash": "^4.17.11",
    "mini-css-extract-plugin": "^0.5.0",
    "mocha": "^5.2.0",
    "ms": "^2.1.1",
    "nested-property": "0.0.7",
    "node-cron": "^2.0.3",
    "node-sass": "^4.11.0",
    "npm-run-all": "^4.1.5",
    "object-hash": "^1.3.1",
    "prettier": "^1.16.4",
    "prettier-check": "^2.0.0",
    "prettycron": "^0.10.0",
    "prop-types": "^15.7.2",
    "pug": "^2.0.3",
    "pug-loader": "^2.4.0",
    "react": "^16.8.3",
    "react-ace": "^6.4.0",
    "react-cytoscape": "^1.0.6",
    "react-dom": "^16.8.3",
    "react-json-view": "^1.19.1",
    "react-lazylog": "^3.2.0",
    "react-redux": "^5.1.1",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-router-redux": "^5.0.0-alpha.9",
    "react-select": "^2.4.1",
    "react-tabs": "^3.0.0",
    "redux": "^4.0.1",
    "redux-actions": "^2.6.5",
    "redux-thunk": "^2.3.0",
    "rimraf": "^2.6.3",
    "sass-loader": "^7.1.0",
    "sinon": "^7.2.7",
    "style-loader": "^0.23.1",
    "styled-components": "^4.1.3",
    "vulpes": "^0.22.1",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.2.3",
    "webpack-livereload-plugin": "^2.2.0"
  },
  "peerDependencies": {
    "express": "4.*",
    "vulpes": ">= 0.21.0 < 1"
  },
  "dependencies": {
    "body-parser": "^1.18.3",
    "pify": "^4.0.1",
    "url-join": "^4.0.0"
  }
}
