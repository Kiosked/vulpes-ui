{
  "name": "vulpes-ui",
  "version": "0.1.0",
  "description": "Web-based GUI for Vulpes",
  "main": "source/index.js",
  "scripts": {
    "build:development": "webpack --mode development --config webpack.config.js --watch",
    "dev": "concurrently --kill-others 'npm run build:development' 'node ./scripts/dev.js'",
    "format": "prettier --write '{source,test}/**/*.js'",
    "precommit": "lint-staged",
    "test": "npm run test:format",
    "test:format": "prettier-check '{source,test}/**/*.js'"
  },
  "lint-staged": {
    "{source,test}/**/*.js": [
      "prettier --write",
      "git add"
    ]
  },
  "engines": {
    "node": ">=8"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Kiosked/vulpes-ui.git"
  },
  "keywords": [
    "vulpes",
    "job",
    "management",
    "queue",
    "task",
    "gui"
  ],
  "author": "Perry Mitchell <perry@kiosked.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Kiosked/vulpes-ui/issues"
  },
  "homepage": "https://github.com/Kiosked/vulpes-ui#readme",
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.2",
    "concurrently": "^4.0.1",
    "express": "^4.16.3",
    "file-loader": "^2.0.0",
    "history": "^4.7.2",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^0.14.3",
    "lint-staged": "^7.2.2",
    "prettier": "^1.14.2",
    "prettier-check": "^2.0.0",
    "pug": "^2.0.3",
    "pug-loader": "^2.4.0",
    "react": "^16.5.0",
    "react-dom": "^16.5.0",
    "react-redux": "^5.0.7",
    "react-router": "^4.3.1",
    "react-router-redux": "^5.0.0-alpha.9",
    "redux": "^4.0.0",
    "redux-actions": "^2.6.1",
    "redux-thunk": "^2.3.0",
    "styled-components": "^3.4.6",
    "vulpes": "^0.7.1",
    "webpack": "^4.18.0",
    "webpack-cli": "^3.1.0"
  },
  "peerDependencies": {
    "express": "4.*",
    "vulpes": "0.*"
  }
}
