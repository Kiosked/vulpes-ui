{
  "name": "vulpes-ui",
  "version": "0.1.0",
  "description": "Web-based GUI for Vulpes",
  "main": "source/index.js",
  "scripts": {
    "build": "webpack --progress --mode production --config webpack.config.js",
    "build:development": "webpack --watch --progress --mode development --config webpack.config.js",
    "dev": "node ./scripts/dev.js",
    "format": "prettier --write '{source,test}/**/*.js'",
    "precommit": "lint-staged",
    "test": "run-s test:unit test:integration test:format",
    "test:format": "prettier-check '{source,test}/**/*.js'",
    "test:integration": "mocha -r test/index.js 'test/integration/**/*.spec.js'",
    "test:unit": "mocha -r test/index.js 'test/unit/**/*.spec.js'",
    "watch": "npm run build:development"
  },
  "lint-staged": {
    "{source,test}/**/*.js": [
      "prettier --write",
      "git add"
    ]
  },
  "engines": {
    "node": ">=8"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Kiosked/vulpes-ui.git"
  },
  "keywords": [
    "vulpes",
    "job",
    "management",
    "queue",
    "task",
    "gui"
  ],
  "author": "Perry Mitchell <perry@kiosked.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Kiosked/vulpes-ui/issues"
  },
  "homepage": "https://github.com/Kiosked/vulpes-ui#readme",
  "devDependencies": {
    "@babel/core": "^7.1.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "@blueprintjs/core": "^3.6.1",
    "babel-loader": "^8.0.2",
    "babel-plugin-jsx-control-statements": "^3.2.8",
    "brace": "^0.11.1",
    "chai": "^4.1.2",
    "copy-webpack-plugin": "^4.5.2",
    "css-loader": "^1.0.0",
    "cytoscape": "^3.2.16",
    "express": "^4.16.3",
    "file-loader": "^2.0.0",
    "history": "^4.7.2",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^0.14.3",
    "lint-staged": "^7.3.0",
    "lodash": "^4.17.11",
    "mini-css-extract-plugin": "^0.4.3",
    "mocha": "^5.2.0",
    "node-sass": "^4.9.3",
    "npm-run-all": "^4.1.3",
    "prettier": "^1.14.3",
    "prettier-check": "^2.0.0",
    "pug": "^2.0.3",
    "pug-loader": "^2.4.0",
    "react": "^16.5.2",
    "react-ace": "^6.2.0",
    "react-cytoscape": "^1.0.6",
    "react-dom": "^16.5.2",
    "react-hot-loader": "^4.3.11",
    "react-loader-spinner": "^2.0.6",
    "react-redux": "^5.0.7",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-router-redux": "^5.0.0-alpha.9",
    "react-select": "^2.1.0",
    "react-tabs": "^2.3.0",
    "redux": "^4.0.0",
    "redux-actions": "^2.6.1",
    "redux-thunk": "^2.3.0",
    "rimraf": "^2.6.2",
    "sass-loader": "^7.1.0",
    "sinon": "^6.3.4",
    "style-loader": "^0.23.0",
    "styled-components": "^3.4.9",
    "url-join": "^4.0.0",
    "vulpes": "^0.11.0",
    "webpack": "^4.19.1",
    "webpack-cli": "^3.1.0",
    "webpack-dev-middleware": "^3.4.0",
    "webpack-hot-middleware": "^2.24.3"
  },
  "peerDependencies": {
    "express": "4.*",
    "vulpes": "0.*"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "body-parser": "^1.18.3"
  }
}
