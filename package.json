{
  "name": "vulpes-ui",
  "version": "0.15.0",
  "description": "Web-based GUI for Vulpes",
  "main": "source/index.js",
  "scripts": {
    "build": "webpack --progress --mode production --config webpack.config.js",
    "build:development": "webpack --watch --mode development --config webpack.config.js",
    "dev": "RELOAD=yes concurrently --kill-others -n 'server,webpack' 'node ./scripts/dev.js' 'npm run build:development'",
    "format": "prettier --write '{source,test}/**/*.js'",
    "prepublishOnly": "npm run build",
    "test": "run-s test:unit test:integration test:format",
    "test:format": "prettier-check '{source,test}/**/*.js'",
    "test:integration": "mocha -r test/index.js 'test/integration/**/*.spec.js'",
    "test:unit": "mocha -r test/index.js 'test/unit/**/*.spec.js'",
    "watch": "npm run build:development"
  },
  "files": [
    "dist/**/*",
    "source/*.js"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "{source,test}/**/*.js": [
      "prettier --write"
    ]
  },
  "engines": {
    "node": ">=8"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Kiosked/vulpes-ui.git"
  },
  "keywords": [
    "vulpes",
    "job",
    "management",
    "queue",
    "task",
    "gui"
  ],
  "author": "Perry Mitchell <perry@kiosked.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Kiosked/vulpes-ui/issues"
  },
  "homepage": "https://github.com/Kiosked/vulpes-ui#readme",
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-function-bind": "^7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.8.3",
    "@babel/preset-env": "^7.8.4",
    "@babel/preset-react": "^7.8.3",
    "@blueprintjs/core": "^3.23.1",
    "@buttercup/channel-queue": "^0.5.0",
    "axios": "^0.19.2",
    "babel-loader": "^8.0.6",
    "babel-plugin-jsx-control-statements": "^4.0.0",
    "brace": "^0.11.1",
    "chai": "^4.2.0",
    "classnames": "^2.2.6",
    "concurrently": "^5.1.0",
    "copy-webpack-plugin": "^5.1.1",
    "css-loader": "^3.4.2",
    "debounce": "^1.2.0",
    "express": "^4.17.1",
    "file-loader": "^5.1.0",
    "history": "^4.10.1",
    "html-webpack-plugin": "^3.2.0",
    "human-date": "^1.4.0",
    "husky": "^4.2.3",
    "lint-staged": "^10.0.7",
    "lodash": "^4.17.15",
    "mini-css-extract-plugin": "^0.9.0",
    "mocha": "^7.0.1",
    "ms": "^2.1.2",
    "nested-property": "^2.0.0",
    "node-cron": "^2.0.3",
    "node-sass": "^4.13.1",
    "npm-run-all": "^4.1.5",
    "object-hash": "^2.0.3",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "prettier": "^1.19.1",
    "prettier-check": "^2.0.0",
    "prettycron": "^0.10.0",
    "prop-types": "^15.7.2",
    "pug": "^2.0.4",
    "pug-loader": "^2.4.0",
    "react": "^16.12.0",
    "react-ace": "^6.6.0",
    "react-dom": "^16.12.0",
    "react-force-graph": "^1.30.1",
    "react-json-view": "^1.19.1",
    "react-lazylog": "^4.5.1",
    "react-paginate": "^6.3.2",
    "react-redux": "^5.1.1",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-router-redux": "^5.0.0-alpha.9",
    "react-select": "^3.0.8",
    "react-tabs": "^3.1.0",
    "redux": "^4.0.5",
    "redux-actions": "^2.6.5",
    "redux-thunk": "^2.3.0",
    "rimraf": "^3.0.2",
    "sass-loader": "^8.0.2",
    "sinon": "^9.0.0",
    "style-loader": "^1.1.3",
    "styled-components": "^5.0.1",
    "time-limit-promise": "^1.0.4",
